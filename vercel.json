{
    "builds": [
      {
        "src": "api/app.py",
        "use": "@vercel/python"
      },
      {
        "src": "templates/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "api/app.py" },
        { "src": "/about", "dest": "api/app.py" },
        { "src": "/contact", "dest": "api/app.py" },
        { "src": "/login", "dest": "api/app.py" },
        { "src": "/register", "dest": "api/app.py" },
        { "src": "/other-route", "dest": "api/app.py" }
        
    ]
  }
  